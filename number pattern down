#include <stdio.h>
int main()
{
    int i, j, rows,k,l,m,n,r;

    printf("Enter the number of rows: ");
    scanf("%d",&n);

    for(r=1;r<=n;r++)
    {
        for(j=1,l=1;j<=r;j++)
        {
            printf("%d",l);
            l=l+1;
        }
    printf("\n");
    }















    return 0;
}
